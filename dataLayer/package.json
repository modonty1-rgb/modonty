{
  "name": "@modonty/database",
  "version": "0.1.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "prisma:generate": "prisma generate --schema=./prisma/schema/schema.prisma",
    "prisma:push": "prisma db push --schema=./prisma/schema/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema/schema.prisma",
    "prisma:format": "prisma format --schema=./prisma/schema/schema.prisma",
    "prisma:validate": "prisma validate --schema=./prisma/schema/schema.prisma",
    "seed": "tsx prisma/seed.ts",
    "seed:clear": "tsx prisma/seed.ts --clear",
    "dedupe-before-push": "tsx scripts/dedupe-before-push.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "mongodb": "^6.0.0"
  },
  "devDependencies": {
    "prisma": "^6.19.1",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
